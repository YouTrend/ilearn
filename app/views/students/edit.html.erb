    <div class="col-lg-12">
        <h2 class="page-header">修改學生</h2>
    </div>
    <!-- /.col-lg-12 -->
  </div>
  <div class="row">
    <div class="col-lg-12">
      <div class="panel panel-default">
        <div class="panel-heading">
          學生資料
        </div>
        <%= form_for @student, url: student_path(@student), method: :patch do |f| %>
          <%= render "students/edit", f: f %>
        <% end %>
      </div>
    </div>
  </div>
</div>



